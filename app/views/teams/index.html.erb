<h1><%= @user.username%>'s Fantasy Football Teams</h1>
    <ul>
        <% @user.teams.each do |team| %>
            <li><a href="/teams/<%= team.id %>"><%= team.name %></a>
            <a href="/teams/<%= team.id %>/edit">edit team</a> / 
            <a href="/teams/<%= team.id %>/delete">delete team</a>
            </li>
        <% end %>
    </ul>


<br><br><a href='/teams/new'>Click here to create a new team</a>


